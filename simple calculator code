#include<stdio.h>
int main() {
    char op;
    int num1, num2;
    printf("enter the operator: ");
    scanf("%c",&op);
    printf("enter the num1: ");
    scanf("%d",&num1);
    printf("enter the num2: ");
    scanf("%d",&num2);
    if(op=='+'){
        printf("result=%d",num1+num2);
    }
  else   if(op=='-'){
        printf("result=%d",num1-num2);
    }
  else   if(op=='*'){
        printf("result=%d",num1*num2);
    }
     else   if(op=='/'){
        printf("result=%d",num1/num2);
    }
     else {
        printf("operator is not correct");
    }
    return 0;
}
